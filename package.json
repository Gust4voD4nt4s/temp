    "@babel/types": "^7.24.7",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/config": "^3.3.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.16",
    "@types/reflect-metadata": "^0.1.0",
    "eslint": "9.x",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.15.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
    "start": "node build/src/app.js",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "rm -rf build && yarn build && yarn typeorm migration:generate ./src/migrations/added-entity -d ./src/utils/data-source.ts",
    "db:push": "rm -rf build && yarn build && yarn typeorm migration:run -d src/utils/data-source.ts",
    "orm": "ts-node-dev ./node_modules/typeorm/cli.js"
  }
